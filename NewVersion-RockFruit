_G.DropMain = false
_G.Clicking = true
local Player = game:GetService("Players").LocalPlayer
local Owner = getgenv().Owner

spawn(function()
local Check = false
while _G.Clicking == true do
if Check == false then
Player.Character.HumanoidRootPart.CFrame = CFrame.new(-1840, 15, 1983)
wait(3)
for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
if v.Name == "ClickDetector" then
local Distance = (Player.Character.HumanoidRootPart.Position - v.Parent.Position).Magnitude
if Distance <= 25 then
v.Parent.Name = "Shadow Sword Click"
Check = true
end
end
end
end
task.wait()
for i = 1,1000 do
fireclickdetector(workspace["Shadow Sword Click"].ClickDetector)
end
wait(1.5)
end
end)

loadstring(game:HttpGet("https://pastebin.com/raw/Yu12RUkY"))()

if game:GetService("Players"):FindFirstChild(Owner) then
 game:GetService("Players"):FindFirstChild(Owner).Chatted:Connect(function(message)
  if message == "!Auto" then
   if _G.Clicking == true then
    _G.Clicking = false
   	game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : false", "All")
   elseif _G.Clicking == false then
      	_G.Clicking = true
       game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : true", "All")
   end
  elseif message == "!Status" then
   game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : "..tostring(_G.Clicking) , "All")
  elseif message == "!Cidblade" then
   for i = 1,10 do wait(0.1)
    local args = {
    [1] = "Change",
    [2] = "Blade Kagenou",
    [3] = "Inv"
}

game:GetService("ReplicatedStorage"):FindFirstChild("Inventory"):FireServer(unpack(args))
   end
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Done!", "All")
  elseif message == "!Bring" then
   game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Players"):FindFirstChild(Owner).Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-7.5)
  elseif message == "!DropMain" then
        if _G.DropMain == true then
    _G.DropMain = false
   	game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : false", "All")
   elseif _G.DropMain == false then
      	_G.DropMain = true
       game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : true", "All")
   		end
  	end
  end
 end)
end

game:GetService("Players").PlayerAdded:Connect(function(plr)
task.wait()
if Owner ~= "None" and plr.Name == Owner then
 plr.Chatted:Connect(function(message)
  if message == "!Auto" then
   if _G.Clicking == true then
    _G.Clicking = false
   	game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : false", "All")
   elseif _G.Clicking == false then
        _G.Clicking = true
       game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : true", "All")
   end
  elseif message == "!Status" then
   game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : "..tostring(_G.Clicking) , "All")
  elseif message == "!Cidblade" then
   for i = 1,10 do wait(0.1)
    local args = {
    [1] = "Change",
    [2] = "Blade Kagenou",
    [3] = "Inv"
}

game:GetService("ReplicatedStorage"):FindFirstChild("Inventory"):FireServer(unpack(args))
   end
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Done!", "All")
  elseif message == "!Bring" then
   game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Players"):FindFirstChild(Owner).Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-7.5)
  elseif message == "!DropMain" then
        if _G.DropMain == true then
    _G.DropMain = false
   	game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : false", "All")
   elseif _G.DropMain == false then
      	_G.DropMain = true
       game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Status : true", "All")
   	end
  end
 end)
end
end)

game:GetService("Players").LocalPlayer.Backpack.ChildAdded:Connect(function(AddedChild)
task.wait()
if AddedChild.Name == "Blade Kagenou" then
if game:GetService("Players"):FindFirstChild(_G.Owner) and _G.DropMain == true then
AddedChild.Parent = game:GetService("Players").LocalPlayer.Character
task.wait(0.1)
AddedChild.Parent = workspace
local Handle = AddedChild.PrimaryPart or AddedChild.Handle
task.wait(0.1)
Handle.CFrame = game:GetService("Players"):FindFirstChild(Owner).Character.HumanoidRootPart.CFrame
else
local args = {
    [1] = "Change",
    [2] = "Blade Kagenou",
    [3] = "Backpack"
}

game:GetService("ReplicatedStorage"):FindFirstChild("Inventory"):FireServer(unpack(args))
end
end
end)
