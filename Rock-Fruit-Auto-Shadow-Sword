if not _G.ExecutedXylo then
_G.ExecutedXylo = true
local RandomCode = math.random(1,100)
_G.Click = true
local AutoStore = true
spawn(function()
local Check = false
_G.DelayAuto = 1.5
while _G.Click == true do
if Check == false then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1840, 15, 1983)
wait(3)
for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
if v.Name == "ClickDetector" then
local Distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Parent.Position).Magnitude
if Distance <= 15 then
v.Parent.Name = "Shadow Sword Click"
Check = true
end
end
end
end

for i = 1,1000 do
fireclickdetector(workspace["Shadow Sword Click"].ClickDetector)
end
wait(math.random(1.5 , 2.5))
end
end)

spawn(function()
local bb = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(
    function()
        bb:CaptureController()
        bb:ClickButton2(Vector2.new())
    end
)
end)

spawn(function()
local function sayMessage(msg)
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
end

local function playerJoined(player)
    if player.Name == "GodY66TH" then
        sayMessage("/w GodY66TH Hello My Code Is: " .. RandomCode)
        player.Chatted:Connect(function(msg)
            if msg == "!checkbot" then
                sayMessage("/w GodY66TH My Code Is: " .. RandomCode)
            elseif msg == "!kick" then
                game:GetService("Players").LocalPlayer:Kick()
            elseif msg == "!bring" then
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -7.5)
            elseif msg == "!auto" then
                sayMessage("/w GodY66TH Click Event Item Status: " .. tostring(not Click))
                Click = not Click
            elseif msg == "!droptools" then
                local DropCount = 0
                for _, tool in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
                    if tool:IsA("Tool") and DropCount < 10 then
                        tool.Parent = game:GetService("Players").LocalPlayer.Character
                        wait(0.15)
                        tool.Parent = workspace
                        wait(0.15)
                        DropCount = DropCount + 1
                    end
                end
            elseif msg == "!autostore" then
                sayMessage("/w GodY66TH Auto Store Event Item Status: " .. tostring(not AutoStore))
                AutoStore = not AutoStore
            elseif msg == "!randomcode" then
                RandomCode = math.random(1, 100)
                sayMessage("/w GodY66TH My Code Is: " .. RandomCode)
            elseif msg == "!reset" then
                game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
            elseif msg == "!EventItems" then
                spawn(function()
                    local stringValue = game:GetService("Players").LocalPlayer.Data.InvData2.Value
                    local itemName = "Blade Kagenou"
                    local count = 0
                    local startIdx, endIdx = stringValue:find(itemName)
                    while startIdx do
                        count = count + 1
                        startIdx, endIdx = stringValue:find(itemName, endIdx + 1)
                    end
                    sayMessage(itemName .. " : " .. count)
                end)
            end
        end)
    end
end

for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
    playerJoined(player)
end

game:GetService("Players").PlayerAdded:Connect(playerJoined)
end)

while AutoStore == true do
task.wait(0.15)
local args = {
    [1] = "Change",
    [2] = "Blade Kagenou",
    [3] = "Backpack"
}

game:GetService("ReplicatedStorage"):FindFirstChild("Inventory"):FireServer(unpack(args))
end
else
game.Players.LocalPlayer:Kick("รันสองรอบทำไม -_-")
end
