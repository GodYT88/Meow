spawn(function()
local Check = false
while true do
if Check == false then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1840, 15, 1983)
wait(3)
for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
if v.Name == "ClickDetector" then
local Distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Parent.Position).Magnitude
if Distance <= 15 then
v.Parent.Name = "Shadow Sword Click"
Check = true
end
end
end
end

for i = 1,1000 do
fireclickdetector(workspace["Shadow Sword Click"].ClickDetector)
end
wait(1.5)
end
end)


loadstring(game:HttpGet("https://pastebin.com/raw/Yu12RUkY"))()
task.wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1840, 15, 1983)
task.wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true

game:GetService("Players").LocalPlayer.Backpack.ChildAdded:Connect(function(AddedChild)
task.wait()
if AddedChild.Name == "Blade Kagenou" then
local HttpService = game:GetService("HttpService")
        local Webhook_URL = "https://discord.com/api/webhooks/1193238814190227617/Q8N0YB9LjnbJbTnA7gI7bPZ77F82De7YezTIRb6OiH3OmF9nHSfy_zHh8jm1qZCFDd25"

        local response = request({
            Url = Webhook_URL,
            Method = 'POST',
            Headers = {
                ['Content-Type'] = 'application/json'
            },
            Body = HttpService:JSONEncode({
                ["content"] = "",
                ["embeds"] = {{
                    ["title"] = "",
                    ["description"] = "Rock Fruit Kagenou Blade Notify",
                    ["type"] = "rich",
                    ["color"] = tonumber(0xffffff),
                    ["fields"] = {
                        {
                            ["name"] = "",
                            ["value"] = tostring(game:GetService("Players").LocalPlayer.Name .. " Has Got ".. "Blade Kagenou"),
                            ["inline"] = true
                        }
                    }
                }}
            })
        })
local args = {
    [1] = "Change",
    [2] = "Blade Kagenou",
    [3] = "Backpack"
}

game:GetService("ReplicatedStorage"):FindFirstChild("Inventory"):FireServer(unpack(args))
end
end)
