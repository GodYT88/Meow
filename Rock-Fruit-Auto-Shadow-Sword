if not _G.ExecutedXylo then
_G.ExecutedXylo = true
local RandomCode = math.random(1,10000)
_G.Click = true
local AutoStore = true
spawn(function()
local Check = false
_G.DelayAuto = 1.5
while _G.Click == true do
if Check == false then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1840, 15, 1983)
wait(3)
for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
if v.Name == "ClickDetector" then
local Distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Parent.Position).Magnitude
if Distance <= 15 then
v.Parent.Name = "Shadow Sword Click"
Check = true
end
end
end
end

for i = 1,1000 do
fireclickdetector(workspace["Shadow Sword Click"].ClickDetector)
end
wait(math.random(1.5 , 2.5))
end
end)

spawn(function()
wait(0.5)
local ba = Instance.new("ScreenGui")
local ca = Instance.new("TextLabel")
local da = Instance.new("Frame")
local _b = Instance.new("TextLabel")
local ab = Instance.new("TextLabel")
ba.Parent = game.CoreGui
ba.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ca.Parent = ba
ca.Active = true
ca.BackgroundColor3 = Color3.new(0.176471, 0.176471, 0.176471)
ca.Draggable = true
ca.Position = UDim2.new(0.698610067, 0, 0.098096624, 0)
ca.Size = UDim2.new(0, 370, 0, 52)
ca.Font = Enum.Font.SourceSansSemibold
ca.Text = "Anti AFK Script"
ca.TextColor3 = Color3.new(0, 1, 1)
ca.TextSize = 22
da.Parent = ca
da.BackgroundColor3 = Color3.new(0.196078, 0.196078, 0.196078)
da.Position = UDim2.new(0, 0, 1.0192306, 0)
da.Size = UDim2.new(0, 370, 0, 107)
_b.Parent = da
_b.BackgroundColor3 = Color3.new(0.176471, 0.176471, 0.176471)
_b.Position = UDim2.new(0, 0, 0.800455689, 0)
_b.Size = UDim2.new(0, 370, 0, 21)
_b.Font = Enum.Font.Arial
_b.Text = ">~< Meow"
_b.TextColor3 = Color3.new(0, 1, 1)
_b.TextSize = 20
ab.Parent = da
ab.BackgroundColor3 = Color3.new(0.176471, 0.176471, 0.176471)
ab.Position = UDim2.new(0, 0, 0.158377, 0)
ab.Size = UDim2.new(0, 370, 0, 44)
ab.Font = Enum.Font.ArialBold
ab.Text = "Status: Active"
ab.TextColor3 = Color3.new(0, 1, 1)
ab.TextSize = 20
local bb = game:service "VirtualUser"
game:service "Players".LocalPlayer.Idled:connect(
    function()
        bb:CaptureController()
        bb:ClickButton2(Vector2.new())
        ab.Text = "Roblox tried to kick u but i kicked him instead"
        wait(2)
        ab.Text = "Status : Active"
    end
)
end)

spawn(function()
if game:GetService("Players"):FindFirstChild("GodY66TH") then
game:GetService("Players")["GodY66TH"].Chatted:Connect(function(msg)
if msg == "!checkbot" then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w GodY66TH My Code Is : "..RandomCode, "All")
elseif msg == tostring("!kick "..RandomCode) then
game:GetService("Players").LocalPlayer:Kick()
elseif msg == tostring("!bring "..RandomCode) then
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Players")["GodY66TH"].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-7.5)
elseif msg == tostring("!auto "..RandomCode) then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w GodY66TH Click Event Item Status : "..tostring(not _G.Click), "All")
_G.Click = (not _G.Click)
elseif msg == tostring("!droptools "..RandomCode) then
local DropCount = 0
for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
if v.CanBeDropped == true and DropCount < 10 then
v.Parent = game:GetService("Players").LocalPlayer.Character
task.wait(0.15)
v.Parent = workspace
task.wait(0.15)
DropCount = DropCount + 1
end
end
elseif msg == tostring("!autostore "..RandomCode) then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w GodY66TH Auto Store Event Item Status : "..tostring(not AutoStore), "All")
AutoStore = (not AutoStore)
end
end)
else
game:GetService("Players").PlayerAdded:Connect(function(player)
if player.Name == "GodY66TH" then
player.Chatted:Connect(function(msg)
if msg == "!checkbot" then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w GodY66TH My Code Is : "..RandomCode, "All")
elseif msg == tostring("!kick "..RandomCode) then
game:GetService("Players").LocalPlayer:Kick()
elseif msg == tostring("!bring "..RandomCode) then
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Players")["GodY66TH"].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-7.5)
elseif msg == tostring("!auto "..RandomCode) then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w GodY66TH Click Event Item Status : "..tostring(not Click), "All")
Click = (not Click)
elseif msg == tostring("!droptools "..RandomCode) then
local DropCount = 0
for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
if v.CanBeDropped == true and DropCount < 10 then
v.Parent = game:GetService("Players").LocalPlayer.Character
task.wait(0.15)
v.Parent = workspace
task.wait(0.15)
DropCount = DropCount + 1
end
end
elseif msg == tostring("!autostore "..RandomCode) then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w GodY66TH Auto Store Event Item Status : "..tostring(not AutoStore), "All")
AutoStore = (not AutoStore)
end
end)
end
end)
end
end)

while AutoStore == true do
task.wait(0.15)
local args = {
    [1] = "Change",
    [2] = "Blade Kagenou",
    [3] = "Backpack"
}

game:GetService("ReplicatedStorage"):FindFirstChild("Inventory"):FireServer(unpack(args))
end
else
game.Players.LocalPlayer:Kick("รันสองรอบทำไม -_-")
end
